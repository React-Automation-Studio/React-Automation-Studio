









record(ao, "$(device):A1")
{       field(DTYP,"Soft Channel")
        field(DESC, "Amplitude of Sine wave")
        field(EGU, "V")
        field(HOPR, "10000")
        field(DRVH, "10000")
        field(DRVL, "0")
        field(LOPR, "0")
        field(HIHI, "8000")
        field(HIGH, "6000")
        field(LOW, "4000")
        field(LOLO, "2000")
        field(HHSV, "MAJOR")
        field(HSV, "MINOR")
        field(LSV, "MINOR")
        field(LLSV, "MAJOR")
        field(VAL,"5000")
        field(FLNK,"$(device):test5")
}

